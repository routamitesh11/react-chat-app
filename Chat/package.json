{
  "name": "chaat-api",
  "version": "1.0.0",
  "description": "A simple chat api",
  "main": "lib/app.js",
  "scripts": {
    "start": "node ./lib/app.js",
    "compile": "babel -d lib/ src/",
    "run": "babel -d lib/ src/ && node lib/app"
  },
  "author": "Amitesh Rout",
  "license": "ISC",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http": "0.0.0",
    "path": "^0.12.7",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-react": "^7.14.2",
    "mongodb": "^3.2.7"
  }
}
